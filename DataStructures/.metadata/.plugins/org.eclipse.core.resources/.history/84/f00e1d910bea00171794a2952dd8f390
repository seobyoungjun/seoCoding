import java.util.Arrays;
import java.util.Scanner;

public class JesseAandCookies {
	static int opCount = 0;

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		int K = in.nextInt();
		int[] array = new int[N];
		for (int i = 0; i < N; i++)
			array[i] = in.nextInt();
		Arrays.sort(array);
		while (opCount < array.length && mix(array, opCount, K) < K) {
			// 오름차순
			// Arrays.sort(array);
			opCount++;
		}
		if (opCount < array.length)
			System.out.println(opCount + 1);
		else
			System.out.println(-1);
	}

	public static int mix(int[] array, int opCount, int K) {
		if (array[opCount] > K) {
			return K + 1;
		} else {
			array[opCount + 1] = (array[opCount] * 1) + (array[opCount + 1] * 2);
			array[opCount] = 0;
			Arrays.sort(array);
			return array[opCount + 1];
		}

	}
}
